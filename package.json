{
  "name": "@yootil/template",
  "version": "0.1.0",
  "description": "A template for TypeScript-first NPM packages",
  "author": "Matt Balmer <contact@mattbalmer.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/yootil/templatae"
  },
  "scripts": {
    "start": "tsc --watch",
    "compile": "tsc",
    "prepare": "npm test && npm run compile",
    "test": "mocha --require ts-node/register --require tsconfig-paths/register --timeout 60000 -R spec 'tests/**/*.test.ts'"
  },
  "keywords": [
    "template",
    "npm",
    "package",
    "typescript"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.3",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "mock-local-storage": "^1.1.23",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
